#!/bin/sh
CFLAGS=
CPPFLAGS=
unamem=`uname -m`
if [ $unamem != x86_64 ]; then
CC=gcc
else
CC=gcc
LDFLAGS=-static
LIBS=
VERSION=grub-2.12~rc1
fi
rm -rf $VERSION
tar xzf $VERSION.tar.gz
cd $VERSION
patch -p1 < ../../../subprojects/grub.diff
cd ..
mkdir build
cd build
../$VERSION/configure --prefix=`pwd`
make
make install
cd ..
